{
  "name": "surcharge-rules-form",
  "version": "1.0.0",
  "description": "TypeScript-based surcharge rules form application",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc && webpack --config webpack.config.cjs --mode=production",
    "build:ts": "tsc",
    "build:webpack": "webpack --config webpack.config.cjs --mode=production",
    "dev": "webpack serve --config webpack.config.cjs --mode=development --port 3000",
    "dev:ts": "tsc --watch",
    "clean": "rm -rf dist",
    "start": "npm run dev",
    "serve": "webpack serve --config webpack.config.cjs --mode=production"
  },
  "keywords": [
    "typescript",
    "form",
    "surcharge",
    "rules"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "ts-loader": "^9.5.1",
    "html-webpack-plugin": "^5.5.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3"
  },
  "dependencies": {}
}